{
  "name": "russell_server",
  "version": "1.0.0",
  "description": "russell_blog server in node way!",
  "repository": "...",
  "main": "index.js",
  "scripts": {
    "start": "npm run tsc && npm run serve-prod",
    "start-dev": "nodemon -â€”watch 'src/**/*.ts' --exec 'NODE_ENV=development ts-node ./src/index.ts'",
    "serve-dev": "cross-env NODE_ENV=development node build/index.js",
    "serve-prod": "cross-env NODE_ENV=production node build/index.js",
    "tsc": "./node_modules/.bin/tsc",
    "debug": "cross-env NODE_ENV=development node %NODE_DEBUG_OPTION% build/index.js",
    "watch": "./node_modules/.bin/tsc -w",
    "docs": "./node_modules/.bin/apidoc -i src/ -o docs"
  },
